<?xml version="1.0" encoding="utf-8"?>

<!-- 
	快速登录对话框登录按钮

	@author 麦灿标
 -->

<selector xmlns:android="http://schemas.android.com/apk/res/android" >
     <item 
        android:state_pressed="true">
        <shape>
            <corners
        	android:radius="1dip"/>
		    <stroke
		        android:width="1dip"
		        android:color="@color/gray"/>
		    <padding 
		        android:top="5dip"
		        android:left="20dip"
		        android:bottom="5dip"
		        android:right="20dip"/>
		    <solid
		        android:color="@color/gray"/>
        </shape>
    </item>
     <item>
        <shape>
	        <corners
	        	android:radius="1dip"/>
		    <stroke
		        android:width="1dip"
		        android:color="@color/gray"/>
		    <padding 
		        android:top="5dip"
		        android:left="20dip"
		        android:bottom="5dip"
		        android:right="20dip"/>
		    <solid
		        android:color="@color/white"/>
	    </shape>
    </item>

</selector>
